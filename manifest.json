{
  "name": "GitHub Force Render",
  "short_name": "GFR",
  "description": "Force render imgs in commit and PR pages",
  "author": "gecko655",
  "manifest_version": 2,

  "version": "1.0.0.0",
  "background": {
    "persistent": false,
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "content_scripts": [{
    "matches": [
      "https://github.com/*/*/commit/*/files",
      "https://github.com/*/*/pull/*/commits/*",
      "https://github.com/*/*/pull/*/files",
      "https://github.com/*/*/compare/*"
    ],
    "js": [
      "jquery-3.2.1.min.js",
      "content.js"
    ]
  }],
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
